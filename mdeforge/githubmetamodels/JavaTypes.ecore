<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Java"
    nsURI="java" nsPrefix="http://www.clinigrid.org/2010/Capillary/JavaTypes">
  <eSubpackages name="Lang" nsURI="java-lang" nsPrefix="http://www.clinigrid.org/2010/Capillary/JavaTypes/Lang/">
    <eClassifiers xsi:type="ecore:EDataType" name="Number" instanceClassName="java.lang.Number"/>
  </eSubpackages>
  <eSubpackages name="Util" nsURI="java-util" nsPrefix="http://www.clinigrid.org/2010/Capillary/JavaTypes/Util/">
    <eClassifiers xsi:type="ecore:EDataType" name="Currency" instanceClassName="java.util.Currency"/>
    <eClassifiers xsi:type="ecore:EDataType" name="Locale" instanceClassName="java.util.Locale"/>
    <eClassifiers xsi:type="ecore:EDataType" name="TimeZone" instanceClassName="java.util.TimeZone"/>
  </eSubpackages>
  <eSubpackages name="SQL" nsURI="java-sql" nsPrefix="http://www.clinigrid.org/2010/Capillary/JavaTypes/SQL/">
    <eClassifiers xsi:type="ecore:EDataType" name="Blob" instanceClassName="java.sql.Blob"/>
    <eClassifiers xsi:type="ecore:EDataType" name="Clob" instanceClassName="java.sql.Clob"/>
    <eClassifiers xsi:type="ecore:EDataType" name="Date" instanceClassName="java.sql.Date"/>
    <eClassifiers xsi:type="ecore:EDataType" name="Time" instanceClassName="java.sql.Time"/>
    <eClassifiers xsi:type="ecore:EDataType" name="Timestamp" instanceClassName="java.util.Timestamp"/>
  </eSubpackages>
</ecore:EPackage>
