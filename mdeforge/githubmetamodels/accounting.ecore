    <eStructuralFeatures xsi:type="ecore:EReference" name="order" lowerBound="1" eType="#//Order"
        eOpposite="#//Order/deliverables"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Invoice">
