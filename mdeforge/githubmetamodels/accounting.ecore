    <eStructuralFeatures xsi:type="ecore:EAttribute" name="pricePerUnit" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Deliverable">
