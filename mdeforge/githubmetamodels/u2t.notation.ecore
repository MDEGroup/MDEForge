<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="u2tnotation"
    nsURI="http://www.eclipse.org/mdt/uml2tools/gmf/notation/ext/2008" nsPrefix="uml2tools.notation">
  <eClassifiers xsi:type="ecore:EClass" name="U2TDiagramCanonicalStyle" eSuperTypes="../../org.eclipse.gmf.runtime.notation/model/notation.ecore#//CanonicalStyle">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="syncNodes" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Replacement for CanonicalStyle for diagram nodes. Provides detailed information about synchronization mode. Allows to force diagram to synchronize nodes/links only, or select specific \&quot;synchronized\&quot; visual IDs"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="syncLinks" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nodeTypesToIgnore" upperBound="-1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="linkTypesToIgnore" upperBound="-1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
</ecore:EPackage>
