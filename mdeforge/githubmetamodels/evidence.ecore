<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="evidence"
    nsURI="http:///org/generationcp/model/core/feature/evidence.ecore" nsPrefix="org.generationcp.model.core.feature.evidence">
  <eClassifiers xsi:type="ecore:EClass" name="Evidence" eSuperTypes="#//EvidenceCode">
    <eStructuralFeatures xsi:type="ecore:EReference" name="observationTime" eType="ecore:EClass value.ecore#//TimeValue"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="score" eType="ecore:EClass value.ecore#//Value"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="supportingSource" eType="ecore:EClass identification.ecore#//SimpleIdentifier"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="curator" eType="ecore:EClass organization.ecore#//Contact"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EvidenceCode" eSuperTypes="ontology.ecore#//SimpleOntologyTerm">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="comment" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
